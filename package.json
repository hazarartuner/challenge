{
  "name": "challenge",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mehmet Hazar Artuner <hazar.artuner@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open --hotOnly --config webpack.dev.js",
    "test": "jest --watch",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "eslint ./src -c .eslintrc.js --ext js,jsx"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "babel-loader": "8.0.6",
    "babel-preset-react-app": "9.0.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "10.0.0-0",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  },
  "dependencies": {
    "babel-eslint": "10.0.3",
    "classnames": "2.2.6",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "webpack": "4.41.2",
    "webpack-merge": "4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|svg|ttf|woff|woff2|vtt|srt)$": "jest-transform-stub"
    }
  }
}
